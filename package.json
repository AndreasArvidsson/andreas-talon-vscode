{
	"name": "andreas-talon",
	"displayName": "Andreas talon",
	"description": "VSCode extension used by Talon Voice",
	"version": "1.0.0",
	"publisher": "andreas-arvidsson",
	"license": "MIT",
	"main": "./src/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/AndreasArvidsson/vscode-talon-extension"
	},
	"categories": [
		"Other"
	],
	"engines": {
		"vscode": "^1.53.0"
	},
	"activationEvents": [
		"*"
	],
	"extensionKind": [
		"ui"
	],
	"contributes": {
		"configuration": {
			"title": "Andreas Talon",
			"properties": {
				"andreas-talon.enableJump": {
					"type": "boolean",
					"default": true,
					"description": "Enable or disable jump functionality"
				}
			}
		}
	},
	"scripts": {
		"build": "vsce package"
	},
	"devDependencies": {}
}