{
    "name": "andreas-talon",
    "displayName": "Andreas Talon",
    "description": "VSCode extension used by Talon Voice",
    "version": "3.3.0",
    "publisher": "AndreasArvidsson",
    "license": "MIT",
    "main": "./out/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/AndreasArvidsson/andreas-vscode"
    },
    "engines": {
        "vscode": "^1.68.0"
    },
    "extensionKind": [
        "workspace"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": []
    },
    "scripts": {
        "build": "vsce package",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "prettier": "prettier --write .",
        "watch": "tsc -watch -p ./"
    },
    "extensionDependencies": [
        "vscode.git",
        "pokey.parse-tree"
    ],
    "dependencies": {
        "gitignore-parser": "^0.0.2"
    },
    "devDependencies": {
        "@types/gitignore-parser": "^0.0.0",
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "^16.11.3",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "eslint": "^7.21.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "prettier": "^2.8.7",
        "typescript": "^4.2.2"
    }
}
